<% provide :title, "Profile for #{@user.email}" %><%= @user.email %><p><%= render 'follow_form' if signed_in? %><% if @user.microposts.any? %>	<h1>User has <%= pluralize(@user.microposts.count, 'micropost') %></h1>	<ol>		<%= render @microposts %>		</ol>	<%= will_paginate @microposts %><% end %>